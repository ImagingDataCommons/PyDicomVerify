{
    "version": 1,
    "formatters": {
        "date_time": {
            "format": "%(asctime)s-<%(levelname)-3.3s> %(module)-16.16s...py[%(lineno)03d] -> %(funcName)-16.16s- %(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "date_time",
            "stream": "ext://sys.stdout"
        },
        "file": {
            "class": "logging.FileHandler",
            "formatter": "date_time",
            "mode": "w",
            "filename": "./Logs/log[01-09-2020][17-32-30].log"
        }
    },
    "loggers": {
        "simple": {
            "level": "INFO",
            "handlers": [
                "console",
                "file"
            ],
            "propagate": "no"
        }
    },
    "root": {
        "level": "DEBUG",
        "handlers": [
            "file"
        ],
        "propagate": "no"
    }
}