{
    "version": 1,
    "formatters": {
        "date_time": {
            "format": "%(asctime)s-<%(levelname)-3.3s> %(processName)s[%(process)05d][%(threadName)-10.10s] %(module)-16.16s...py[%(lineno)04d] -> %(funcName)-16.16s- %(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "formatter": "date_time",
            "stream": "ext://sys.stdout"
        },
        "file": {
            "class": "logging.FileHandler",
            "formatter": "date_time",
            "mode": "w",
            "filename": "./Logs/log[02-10-2020][16-52-49].log"
        }
    },
    "loggers": {
        "file": {
            "level": "INFO",
            "handlers": [
                "file"
            ],
            "propagate": "no"
        },
        "console": {
            "level": "INFO",
            "handlers": [
                "console"
            ],
            "qualname": "console",
            "propagate": "no"
        },
        "console_and_file": {
            "level": "INFO",
            "handlers": [
                "console",
                "file"
            ],
            "propagate": "no"
        }
    },
    "root": {
        "level": "INFO",
        "handlers": [
            "file"
        ],
        "propagate": "no"
    }
}